#!/usr/bin/env ruby

p User.all

p Comment.id.all

p Post.all

p Category.all

p Tag.all

p User.first.comments

p User.first.comments << Comment.first

p Category.first.posts << Post.last

p User.first.comments = [Comment.first, Comment.second]

p Comment.first.author

p Post.first.comments

p Comment.first.parent_post

p Category.last.posts.last.destroy

p User.last.authored_posts

p User.last.authored_post_ids

p User.first.authored_posts = [Post.first, Post.second]

p Post.second.authors

p Post.first.authors = [User.first, User.second]

p Post.last.author_ids = [User.first.id, User.second.id]

p Tag.last.tagged_posts

p Tag.second.tagged_post_ids << Post.last.id

p Post.last.tag_ids << Tag.last.id

p Post.second.tags
